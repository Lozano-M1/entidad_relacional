table users {
    id serial [pk]
    username varchar [unique] 
    email varchar
    nombre varchar
}

table post {
    id serial [pk]
    user_id int 
    title varchar
    body text
    created_at timestamp
}

table perfil {
    user_id int [pk]
    perfil_picture varchar
    nombre varchar 
}

Ref: users.id > post.user_id 
Ref: users.id > perfil.user_id

